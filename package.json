{
  "name": "simple-docs",
  "version": "1.0.0",
  "description": "A markdown documentation parser, usinf marked to diplay markdown pages as html.",
  "main": "index.js",
  "scripts": {
    "release": "semantic-release",
    "test": "jest",
    "build:sass": "node compile-sass.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.1.7",
    "@semantic-release/npm": "^12.0.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.5",
    "jest": "^29.6.4",
    "node-sass": "^9.0.0",
    "semantic-release": "^24.1.0",
    "supertest": "^6.3.3"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/api/*.js"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ],
    "coverageDirectory": "coverage"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dry-run": false
